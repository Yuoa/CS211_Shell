[TRACE TEST - 14]
=========================(ref)==========================
#
# trace14.txt - Simple error handling
#
tsh> tsh> ./bogus
tsh> ./bogus: Command not found
tsh> tsh> ./myspin 4 &
tsh> [1] (25804) ./myspin 4 &
tsh> tsh> fg
tsh> fg command requires PID or %jobid argument
tsh> tsh> bg
tsh> bg command requires PID or %jobid argument
tsh> tsh> fg a
tsh> fg: argument must be a PID or %jobid
tsh> tsh> bg a
tsh> bg: argument must be a PID or %jobid
tsh> tsh> fg 9999999
tsh> (9999999): No such process
tsh> tsh> bg 9999999
tsh> (9999999): No such process
tsh> tsh> fg %2
tsh> %2: No such job
tsh> tsh> fg %1
tsh> Job [1] (25804) stopped by signal 20
tsh> tsh> bg %2
tsh> %2: No such job
tsh> tsh> bg %1
tsh> [1] (25804) ./myspin 4 &
tsh> tsh> jobs
tsh> [1] (25804) Running ./myspin 4 &
tsh> 
-----(Our tsh)-----
#
# trace14.txt - Simple error handling
#
tsh> tsh> ./bogus
tsh> ./bogus: Command not found.
tsh> tsh> ./myspin 4 &
tsh> [1] (25821) ./myspin 4 &
tsh> tsh> fg
tsh> Requires PID or %jobid argument after fg
tsh> tsh> bg
tsh> Requires PID or %jobid argument after bg
tsh> tsh> fg a
tsh> Requires PID or %jobid argument after fg
tsh> tsh> bg a
tsh> Requires PID or %jobid argument after bg
tsh> tsh> fg 9999999
tsh> (9999999): No such process
tsh> tsh> bg 9999999
tsh> (9999999): No such process
tsh> tsh> fg %2
tsh> (%2): No such job
tsh> tsh> fg %1
tsh> tsh: pid 25821 killed(20).
tsh> tsh> bg %2
tsh> (%2): No such job
tsh> tsh> bg %1
tsh> [1] (25821) ./myspin 4 &
tsh> tsh> jobs
tsh> [1] (25821) Running ./myspin 4 &
tsh> 
========================================================


[TRACE TEST - 15]
=========================(ref)==========================
#
# trace15.txt - Putting it all together
#
tsh> tsh> ./bogus
tsh> ./bogus: Command not found
tsh> tsh> ./myspin 10
tsh> Job [1] (25838) terminated by signal 2
tsh> tsh> ./myspin 3 &
tsh> [1] (25840) ./myspin 3 &
tsh> tsh> ./myspin 4 &
tsh> [2] (25842) ./myspin 4 &
tsh> tsh> jobs
tsh> [1] (25840) Running ./myspin 3 &
[2] (25842) Running ./myspin 4 &
tsh> tsh> fg %1
tsh> Job [1] (25840) stopped by signal 20
tsh> tsh> jobs
tsh> [1] (25840) Stopped ./myspin 3 &
[2] (25842) Running ./myspin 4 &
tsh> tsh> bg %3
tsh> %3: No such job
tsh> tsh> bg %1
tsh> [1] (25840) ./myspin 3 &
tsh> tsh> jobs
tsh> [1] (25840) Running ./myspin 3 &
[2] (25842) Running ./myspin 4 &
tsh> tsh> fg %1
tsh> tsh> tsh> quit
tsh> 
-----(Our tsh)-----
#
# trace15.txt - Putting it all together
#
tsh> tsh> ./bogus
tsh> ./bogus: Command not found.
tsh> tsh> ./myspin 10
tsh> tsh: pid 25856 killed(2).
tsh: pid 25856 killed(20).
tsh> tsh> ./myspin 3 &
tsh> [2] (25858) ./myspin 3 &
tsh> tsh> ./myspin 4 &
tsh> [3] (25860) ./myspin 4 &
tsh> tsh> jobs
tsh> [1] (25856) Stopped ./myspin 10
[2] (25858) Running ./myspin 3 &
[3] (25860) Running ./myspin 4 &
tsh> tsh> fg %1
tsh> tsh> tsh> jobs
tsh> tsh> tsh> bg %3
tsh> (%3): No such job
tsh> tsh> bg %1
tsh> (%1): No such job
tsh> tsh> jobs
tsh> tsh> tsh> fg %1
tsh> (%1): No such job
tsh> tsh> quit
tsh> 
========================================================


[TRACE TEST - 16]
=========================(ref)==========================
#
# trace16.txt - Tests whether the shell can handle SIGTSTP and SIGINT
#     signals that come from other processes instead of the terminal.
#
tsh> tsh> ./mystop 2
tsh> Job [1] (25872) stopped by signal 20
tsh> tsh> jobs
tsh> [1] (25872) Stopped ./mystop 2
tsh> tsh> ./myint 2
tsh> Job [2] (25875) terminated by signal 2
tsh> 
-----(Our tsh)-----
#
# trace16.txt - Tests whether the shell can handle SIGTSTP and SIGINT
#     signals that come from other processes instead of the terminal.
#
tsh> tsh> ./mystop 2
tsh> kill (tstp) errortsh> tsh> jobs
tsh> tsh> tsh> ./myint 2
tsh> tsh: job [1] (25882) terminated by SIGINT.
tsh> 
========================================================


